{
    "profile": {
      "exit_type": "Normal",
      "restore_on_startup": 4,
      "startup_urls": [
        "https://www.google.com"
      ]
    },
    "browser": {
      "check_default_browser": false
    },
    "intl": {
      "accept_languages": "en-US,en"
    },
    "download": {
      "prompt_for_download": false,
      "default_directory": "/path/to/downloads"
    },
    "safebrowsing": {
      "enabled": true
    }
  }